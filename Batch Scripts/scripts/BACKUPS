'''
Tracker:
Enable/Disable Chroma Toggler Button
Lesson the amount of comms messages outputting
Icon in the corner?

'''



class TimelineFaderWidget:
    def __init__(self, root):
        self.root = root
        self.root.title("Opacity Sliders")
        self.slider_values = {}
        #self.sliders = {}
        #self.create_widgets()
        self.test_button = tk.Button(self.root, text="Gather Timelines", command=self.send_message_timeline_count)
        self.test_button.pack()
    
    def create_widgets(self):
        # Create sliders and timeline names horizontally
        for i in range(1, 11):
            slider = tk.Scale(self.root, from_=1.0, to=0.0, resolution=0.01, orient=tk.VERTICAL, length=150)
            slider.grid(row=0, column=i-1, pady=(10, 0), padx=(5, 5))
            self.sliders[i] = slider  # Store the slider in the dictionary
            self.slider_values[i] = 0  # Initialize slider values
            label = tk.Label(self.root, text=f"TL {i} Opacity")
            label.grid(row=1, column=i-1, padx=(10, 0), pady=(0, 10))
            button = tk.Button(self.root, text="Animate", command=lambda tl=i: self.move_slider(tl))
            button.grid(row=2, column=i-1, padx=(10, 0), pady=(0, 10))
    
    def send_message_opacity(self, timeline_number):
        # Construct the message
        message = '{{"type":"request","cookie":1,"caller-id":"python","command":"set","path":"Timelines/Timeline {}/Opacity","params":{{"opacity":"{}"}}}}'.format(timeline_number, self.slider_values[timeline_number])
        # Construct the payload
        payload = str(len(message)) + ":" + message + ","
        try:
            self.root.master.client_socket.sendall(payload.encode())
            print("Message sent to server:", payload)

        except Exception as e:
            print("Error:", e)
    
    def send_message_timeline_count(self):
        # Construct the message
        message = '{"type": "request","cookie": 1,"caller-id": "CompereCommunicator","command": "get","path": "Timelines","params": {"depth": "1"}}'
        # Construct the payload
        payload = str(len(message)) + ":" + message + ","
        try:
            self.root.master.client_socket.sendall(payload.encode())
            print("Message sent to server:", payload)   

            # Initialize an empty response string
            full_response = ""

            # Receive responses from the server
            while True:
                response = self.root.master.client_socket.recv(1048576).decode()  # Adjust buffer size as needed
                print("Server response:", response) 

                # Append the received response to the full response string
                full_response += response

                # Try parsing the accumulated response as JSON
                try:
                    response_data = json.loads(full_response)
                    if 'Timelines' in response_data.get('data', {}):
                        timeline_names = self.parse_timeline_names(full_response)
                        print("Timeline names:", timeline_names)
                    break  # Exit the loop after parsing the response
                except json.JSONDecodeError:
                    # If the accumulated response is not valid JSON, continue receiving more data
                    continue

        except Exception as e:
            print("Error:", e)

    def parse_timeline_names(self, json_response):
        # Parse the JSON response
        response_data = json.loads(json_response)

        # Extract timeline names
        timeline_names = []
        timelines = response_data.get("data", {}).get("Timelines", {}).get("children", [])
        for timeline in timelines:
            name_value = timeline.get("name", {}).get("value")
            if name_value:
                timeline_names.append(name_value)

        return timeline_names
        
 
    def move_slider(self, timeline_number):
        slider = self.sliders[timeline_number]
        current_value = self.slider_values[timeline_number]
        new_value = 1 - current_value  # Toggle between 0 and 1
        slider.set(new_value)
        self.slider_values[timeline_number] = new_value
        threading.Thread(target=self.send_message_opacity, args=(timeline_number,)).start()


